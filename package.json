{
  "name": "solutionname",
  "version": "1.0.0",
  "description": "SXACLI solution setup with major enhancements",
  "main": "src/main.js",
  "private": true,
  "engines": {
    "node": "11.15.0",
    "npm": "6.7.0"
  },
  "homepage": "https://github.com/macaw-interactive/SXACLI-enhancements/blob/master/readme.md",
  "keywords": [
    "SXA",
    "Sitecore Experience Accelerator",
    "SXACLI"
  ],
  "license": "MIT",
  "scripts": {
    "doc": "rimraf ./jsdoc/ | jsdoc -c ./gulp/jsdoc.json",
    "build-deploy-watch": "npx npm-run-all --serial build-deploy watch",
    "watch": "npx npm-run-all --parallel watch:*",
    "build-deploy": "npx npm-run-all --serial clean build deploy-to-sitecore",
    "build": "npx npm-run-all --serial clean create-fixed-defaulttheme-sass-for-webpack build:sprite-flag build:webpack build:copy-assets-to-cra",
    "build:prod": "npx npm-run-all --serial clean create-fixed-defaulttheme-sass-for-webpack build:sprite-flag build:webpack:prod",
    "clean": "rimraf jsdoc styles scripts images/sprite-flag.png",
    "create-fixed-defaulttheme-sass-for-webpack": "rimraf defaulttheme && copyfiles --all --up 3 ./node_modules/@sxa/Theme/sass/** ./defaulttheme && gulp -- fix-defaulttheme-sass-for-webpack",
    "fix-defaulttheme-sass-for-webpack": "gulp -- fix-defaulttheme-sass-for-webpack",
    "publish-theme": "powershell ./publish-theme.ps1",
    "-- INTERNAL BUILD SCRIPTS --": "---------------------------",
    "build:sprite-flag": "gulp -- sprite-flag",
    "build:webpack": "webpack --mode development --config webpack.config.js",
    "build:webpack:prod": "webpack --mode production --config webpack.config.js",
    "build:copy-assets-to-cra": "ncp styles/ react-components/public/css/",
    "-- INTERNAL DEPLOY SCRIPTS --": "---------------------------",
    "deploy-to-sitecore": "gulp -- deploy-to-sitecore",
    "-- INTERNAL WATCH SCRIPTS --": "---------------------------",
    "watch:core": "gulp -- custom-all-watch",
    "watch:webpack": "webpack --config webpack.config.js --mode development --watch",
    "watch:scriban": "gulp -- watch-scriban"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@sxa/CLI": "^1.0.1",
    "@types/jquery": "^3.3.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-jsonschema-form": "^1.7.1",
    "bootstrap": "^4.4.1",
    "colors": "^1.1.2",
    "copyfiles": "^2.2.0",
    "css-loader": "^3.4.2",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "form-data": "^2.1.4",
    "formstream": "^1.1.0",
    "graceful-fs": "4.2.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^1.2.2",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^3.0.1",
    "gulp-filter": "^5.0.0",
    "gulp-if": "^2.0.2",
    "gulp-prompt": "^0.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^3.1.0",
    "gulp-sass-bulk-import": "^1.0.1",
    "gulp-sass-grapher": "^0.1.4",
    "gulp-sourcemaps": "^2.5.0",
    "gulp-upload": "^0.0.3",
    "gulp-watch": "^4.3.11",
    "gulp.spritesmith": "^6.4.0",
    "inquirer": "^3.0.6",
    "jquery": "^3.4.1",
    "jsdoc": "^3.5.5",
    "latte-carousel": "^1.5.6",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-jsonschema-form": "^1.8.1",
    "request": "^2.81.0",
    "resolve-url-loader": "^3.1.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "typescript": "3.8.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "urllib": "^2.22.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@sxa/celt": "^1.0.1",
    "expose-loader": "^0.7.5",
    "json-loader": "^0.5.7",
    "ncp": "^2.0.0"
  },
  "eslintIgnore": [
    "scripts/pre-optimized-min.js"
  ],
  "eslintConfig": {
    "extends": "eslint:recommended",
    "ecmaVersion": 5,
    "sourceType": "module",
    "rules": {
      "no-alert": 0,
      "no-bitwise": 0,
      "camelcase": 1,
      "curly": 1,
      "eqeqeq": 0,
      "no-eq-null": 0,
      "guard-for-in": 1,
      "no-empty": 1,
      "no-unused-vars": 0,
      "no-console": 0
    },
    "globals": {
      "$": false,
      "jQuery": false,
      "Sitecore": false,
      "XA": false,
      "_": false
    },
    "env": {
      "browser": true,
      "node": true
    }
  }
}
