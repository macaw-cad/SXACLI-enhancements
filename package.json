{
  "name": "solutionname",
  "version": "1.0.0",
  "description": "SXACLI solution setup with major enhancements",
  "main": "src/main.js",
  "private": true,
  "engines": {
    "node": "11.15.0",
    "npm": "6.7.0"
  },
  "homepage": "https://github.com/macaw-interactive/SXACLI-enhancements/blob/master/readme.md",
  "keywords": [
    "SXA",
    "Sitecore Experience Accelerator",
    "SXACLI"
  ],
  "license": "MIT",
  "scripts": {
    "doc": "rimraf ./jsdoc/ | jsdoc -c ./gulp/jsdoc.json",
    "build-deploy-watch": "npx npm-run-all --serial build-deploy watch",
    "watch": "npx npm-run-all --parallel watch:*",
    "build-deploy": "npx npm-run-all --serial clean build deploy-to-sitecore",
    "build": "npx npm-run-all --serial clean build:webpack",
    "build:with-react-stuff-included": "npx npm-run-all --serial clean build:webpack build:copy-assets-to-cra",
    "build:prod": "npx npm-run-all --serial clean build:webpack:prod",
    "clean": "rimraf jsdoc 'Media Library/**/styles' 'Media Library/**/scripts'",
    "publish-theme": "powershell ./publish-theme.ps1",
    "gulp": "gulp",
    "-- INTERNAL BUILD SCRIPTS --": "---------------------------",
    "build:webpack": "webpack --mode development --config config/webpack.config.js",
    "build:webpack:prod": "webpack --mode production --config config/webpack.config.js",
    "build:copy-assets-to-cra": "ncp styles/ react-components/public/css/",
    "-- INTERNAL DEPLOY SCRIPTS --": "---------------------------",
    "deploy-to-sitecore": "gulp --gulpfile tools/gulpfile.js -- deploy-to-sitecore",
    "-- INTERNAL WATCH SCRIPTS --": "---------------------------",
    "watch:gulp": "gulp --gulpfile tools/gulpfile.js-- watch",
    "watch:webpack": "webpack --config config/webpack.config.js --mode development --watch",
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@sxa/Theme": "^1.0.1",
    "@types/jquery": "^3.3.35",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-jsonschema-form": "^1.7.2",
    "bootstrap": "^4.4.1",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^5.1.1",
    "copyfiles": "^2.2.0",
    "css-loader": "^3.5.3",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "form-data": "^2.5.1",
    "formstream": "^1.1.0",
    "got": "^11.0.2",
    "graceful-fs": "4.2.2",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.0.0",
    "gulp-sass-bulk-import": "^1.0.1",
    "gulp-watch": "^5.0.1",
    "inquirer": "^3.0.6",
    "jquery": "^3.5.0",
    "jsdoc": "^3.6.4",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-jsonschema-form": "^1.8.1",
    "request": "^2.81.0",
    "resolve-url-loader": "^3.1.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.0",
    "sxa-defaulttheme": "file:local_modules/sxa-defaulttheme",
    "sxa-types": "file:local_modules/sxa-types",
    "ts-loader": "^6.2.2",
    "typescript": "3.8.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "urllib": "^2.22.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.10"
  },
  "devDependencies": {
    "gulp-spritesmith": "0.0.2",
    "gulp.spritesmith": "^6.11.0",
    "ncp": "^2.0.0"
  },
  "eslintIgnore": [
    "scripts/pre-optimized-min.js"
  ],
  "eslintConfig": {
    "extends": "eslint:recommended",
    "ecmaVersion": 5,
    "sourceType": "module",
    "rules": {
      "no-alert": 0,
      "no-bitwise": 0,
      "camelcase": 1,
      "curly": 1,
      "eqeqeq": 0,
      "no-eq-null": 0,
      "guard-for-in": 1,
      "no-empty": 1,
      "no-unused-vars": 0,
      "no-console": 0
    },
    "globals": {
      "$": false,
      "jQuery": false,
      "Sitecore": false,
      "XA": false,
      "_": false
    },
    "env": {
      "browser": true,
      "node": true
    }
  }
}
